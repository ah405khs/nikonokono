<div id="home-index" class="z-depth-3" style="z-index: 888;">
  <div>
  <ul>
  <li id="assd"></li>
  <li><a href="/showme/index"class="btn">쇼미더 코인</a></li>
</ul>
</div>
</div>

<script>
$(document).ready(function() {
  getCurrentPosd();
  $('#assd').click(function(){
    if (isLoading) {
      alert("위치정보를 받아오는 중입니다...");
    }
});
function getCurrentPosd() {
  if(!navigator.geolocation) {
    alert("브라우저에서 지오로케이션 지원 x");
  }
  function success(position) { // 성공시
    console.log("위치정보 확인 성공!");
    var currentLat = position.coords.latitude;
    var currentLng = position.coords.longitude;
    console.log(currentLat +","+ currentLng);
    $('#assd').html('<a class="btn" href=/cmap/map?lat='+currentLat+'&lng='+currentLng+'>코인 노래방 찾기</a>');
    isLoading = false;
  }
  function fail(err){
    switch (err.code){
      case err.PERMISSION_DENIED:
        msg = "사용자 거부";
      break;
      case err.PERMISSION_UNAVAILABLE:
        msg = "지리정보를 얻을 수 없음";
      break;
      case err.TIMEOUT:
        msg = "시간초과";
        console.log("time out");
      break;
      case err.UNKNOWN_ERROR:
        msg = "알 수 없는 오류 발생";
      break;
    }
    output.innerHTML = "<p>Locating...</p>";

  }

  navigator.geolocation.getCurrentPosition(success, fail);  //현재 위치 정보를 조사하고 성공 실패 했을시 호출되는 함수 설정
}
});
</script>
